<% tag_list = data["ind-all"].meta[tag_name].map{ |t| t } %>

<div class="row primary">
	<div class="columns three border-right">
		<%= partial("partials/page_column_left", :locals => { :tag_list => tag_list, :type => "index" }) %>
	</div>
	<div class="columns nine">
		<div class="subheader">
			<h6 class="kicker"><%= tag %></h6>
			<h4><%= tag_name.titleize %></h4>
			<p class="entry-count"><%= tag_list.length %> entries</p>
		</div>
		<%= partial("partials/entries_list", :locals => { :tag_list => tag_list, :tag_name => tag_name, :tag => tag }) %>
	</div>
</div>

<%# item = data["ind-all"].meta[tag][i] %>